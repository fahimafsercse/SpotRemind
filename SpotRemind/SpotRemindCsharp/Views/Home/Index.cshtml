

@{
    ViewBag.Title = "Index";
}
<h2>Index</h2>

<style>
    #map {
        height: 600px;
        width: 1200px;
    }

    html, body {
        height: 100%;
        margin: 0;
        padding: 0;
    }
</style>
<br />


<div class="row">

    <div id="map"></div>
    <script>
      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
            center: { lat: 23.820715, lng: 90.423123},
            zoom: 15,
          });
        



      }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAyTp6dXmsh2JJfOkPiWPCgdRA7_1z70Ew&callback=initMap"
            async defer></script>

</div>







